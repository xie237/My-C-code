#include<iostream>
#include<vector>
using namespace std;
int main()
{
   int n,m;
   cin>>n>>m;
   vector<vector<int>>matrix(n+1,vector<int>(n+1,0));
   vector<vector<int>>diff(n+2,vector<int>(n+2,0));
   for(int i=0;i<m;i++)
   {
       int x1,y1,x2,y2;
       cin>>x1>>y1>>x2>>y2;
       diff[x1][y1]++;
       diff[x1][y2+1]--;
       diff[x2+1][y1]--;
       diff[x2+1][y2+1]++;
//本身加(左上)，右上右减，左下下减，右下右下加
   }
   for(int i=1;i<=n;i++)
      for(int j=1;j<=n;j++)
      {
          matrix[i][j]=diff[i][j]+matrix[i-1][j]+matrix[i][j-1]-matrix[i-1][j-1];
//diff本身加上左减重复(左上角)
      }
   for(int i=1;i<=n;i++)
      {
          for(int j=1;j<=n;j++)
             {
                 cout<<matrix[i][j]<<" ";
             }
             cout<<endl;
      }
}
