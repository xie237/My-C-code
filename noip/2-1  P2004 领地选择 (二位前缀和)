#include<iostream>
#include<vector>
#include<climits>
using namespace std;

vector<vector<int>> prefix;

int search(int x1, int y1, int x2, int y2)
{
    return prefix[x2 + 1][y2 + 1] - prefix[x1][y2 + 1] - prefix[x2 + 1][y1] + prefix[x1][y1];
}

int main()
{
    int n, m, c;
    cin >> n >> m >> c;
    vector<vector<int>> matrix(n, vector<int>(m));
    prefix.resize(n + 1, vector<int>(m + 1, 0));

    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            cin >> matrix[i][j];

    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            prefix[i + 1][j + 1] = prefix[i][j + 1] + prefix[i + 1][j] - prefix[i][j] + matrix[i][j];

    int result = INT_MIN;
    int x = 0, y = 0;

    for (int i = 0; i <= n - c; i++)
        for (int j = 0; j <= m - c; j++)
        {
            int num = search(i, j, i + c - 1, j + c - 1);
            if (num > result) {
                result = num;
                x = i;
                y = j;
            }
        }

    cout << x + 1 << " " << y + 1;
    return 0;
}
