#include<iostream>
#include<vector>
using namespace std;
int main()
{
    int n,m;
    cin>>n>>m;
    vector<int>studynum(n);
    for(int i=0;i<n;i++)
        cin>>studynum[i];
    vector<int>num(m);
    for(int i=0;i<m;i++)
        cin>>num[i];
    for(int i=0;i<m;i++)
        cout<<studynum[num[i]-1]<<endl;
    return 0;
}
