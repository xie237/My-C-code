#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
#include <string>
using namespace std;

int main() {
    string sentence,temp_sentence;
    getline(cin, sentence);
    for (int i = 0,j=0; i < sentence.size(); i++) 
    {
        if ((sentence[i] >= 'a' && sentence[i] <= 'z') || (sentence[i] >= 'A' && sentence[i] <= 'Z')) { temp_sentence.push_back(sentence[i]);j++; }
    }
    cout << temp_sentence;
    return 0;
}
